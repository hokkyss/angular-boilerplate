{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": false,
    "schematicCollections": ["@angular-eslint/schematics"]
  },
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "configurations": {
            "development": {
              "extractLicenses": false,
              "fileReplacements": [
                {
                  "replace": "projects/app/src/environments/environment.ts",
                  "with": "projects/app/src/environments/environment.development.ts"
                }
              ],
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "budgets": [
                {
                  "maximumError": "1mb",
                  "maximumWarning": "500kb",
                  "type": "initial"
                },
                {
                  "maximumError": "4kb",
                  "maximumWarning": "2kb",
                  "type": "anyComponentStyle"
                }
              ],
              "outputHashing": "all"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "assets": ["projects/app/src/favicon.ico", "projects/app/src/assets"],
            "browser": "projects/app/src/main.ts",
            "index": "projects/app/src/index.html",
            "outputPath": "dist/app",
            "polyfills": ["zone.js"],
            "prerender": true,
            "scripts": [],
            "server": "projects/app/src/main.server.ts",
            "ssr": {
              "entry": "projects/app/server.ts"
            },
            "styles": ["@angular/material/prebuilt-themes/deeppurple-amber.css", "projects/app/src/styles.css"],
            "tsConfig": "projects/app/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app:build"
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "buildTarget": "app:build:development"
            },
            "production": {
              "buildTarget": "app:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": ["projects/app/src/favicon.ico", "projects/app/src/assets"],
            "polyfills": ["zone.js", "zone.js/testing"],
            "scripts": [],
            "styles": ["@angular/material/prebuilt-themes/deeppurple-amber.css", "projects/app/src/styles.css"],
            "tsConfig": "projects/app/tsconfig.spec.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "projects/app",
      "schematics": {
        "@schematics/angular:component": {
          "standalone": false
        },
        "@schematics/angular:directive": {
          "standalone": false
        },
        "@schematics/angular:pipe": {
          "standalone": false
        }
      },
      "sourceRoot": "projects/app/src"
    },
    "common": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "projects/common/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "projects/common/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "project": "projects/common/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/common/**/*.ts", "projects/common/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/common/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "projects/common",
      "sourceRoot": "projects/common/src"
    },
    "directives": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "projects/directives/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "projects/directives/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "project": "projects/directives/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/directives/**/*.ts", "projects/directives/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/directives/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "projects/directives",
      "sourceRoot": "projects/directives/src"
    },
    "interceptors": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "projects/interceptors/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "projects/interceptors/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "project": "projects/interceptors/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/interceptors/**/*.ts", "projects/interceptors/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/interceptors/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "projects/interceptors",
      "sourceRoot": "projects/interceptors/src"
    },
    "pipes": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "projects/pipes/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "projects/pipes/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "project": "projects/pipes/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/pipes/**/*.ts", "projects/pipes/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/pipes/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "projects/pipes",
      "sourceRoot": "projects/pipes/src"
    },
    "utils": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "configurations": {
            "development": {
              "tsConfig": "projects/utils/tsconfig.lib.json"
            },
            "production": {
              "tsConfig": "projects/utils/tsconfig.lib.prod.json"
            }
          },
          "defaultConfiguration": "production",
          "options": {
            "project": "projects/utils/ng-package.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/utils/**/*.ts", "projects/utils/**/*.html"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/utils/tsconfig.spec.json"
          }
        }
      },
      "prefix": "lib",
      "projectType": "library",
      "root": "projects/utils",
      "sourceRoot": "projects/utils/src"
    }
  },
  "version": 1
}
